{
    "code-runner.executorMap": {
        "typescript": "npx ts-node --files --transpile-only",
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
}
